// Ask for mobile and college if they don't already exist
<% if !@user.mobile.present? || !@user.college.present? %>
	$('#user-form').html('<%= j render("users/form") %>')
	$('#user-form').modal({
		backdrop: false,
		keyboard: false,	
	})
	
	$(function() {
	  return $('.inverse-dropdown-select').selectpicker({
	    style: 'btn btn-default',
	    menuStyle: 'dropdown-inverse'
	  });
	});
<% end %>

// update headers
$('#user-session-links').html('<%= j render("layouts/user_session_links") %>')